@model Library.Web.ViewModels.EditLibraryItemViewModel

@if (Model.AudioBook != null)
{
    await Html.RenderPartialAsync("_AudioBookEdit", Model);
}

else if (Model.Book != null)
{
    await Html.RenderPartialAsync("_BookEdit", Model);
}

else if (Model.Dvd != null)
{
    await Html.RenderPartialAsync("_DvdEdit", Model);
}

else
{
    await Html.RenderPartialAsync("_ReferenceBookEdit", Model);
}